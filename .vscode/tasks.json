{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cleanAll",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "buildAll",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "cleanConsole",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/AlphaKopConsole/src/AlphaKop.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildConsole",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/AlphaKopConsole/src/AlphaKop.csproj"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn": [
                "cleanweb"
            ]
        },
        {
            "label": "cleanCaptchaResolverApp",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/CaptchaResolverApp/src/CaptchaResolverApp.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildCaptchaResolverApp",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CaptchaResolverApp/src/CaptchaResolverApp.csproj"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn": [
                "cleanweb"
            ]
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/AlphaKopConsole/src/AlphaKop.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/AlphaKopConsole/src/AlphaKop.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}